          <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.26/angular-resource.min.js"></script>
        
          <link rel="stylesheet"  href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css">
          <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
          <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">


         <!-- <link rel='stylesheet' href='/stylesheets/Profile.css' /> -->

          <meta http-equiv="Content-Type" content="text/html">
        
          <link rel="shortcut icon" href="http://media.gizmodo.co.uk/wp-content/uploads/2014/07/airbnb-logo-230x129.jpg">

         
          <link media="all" type="text/css" rel="stylesheet" href="http://makent.trioangle.com/css/common.css">

          <link media="all" type="text/css" rel="stylesheet" href="http://makent.trioangle.com/css/home.css">

          <link media="all" type="text/css" rel="stylesheet" href="http://makent.trioangle.com/css/themes.css">

          <link media="all" type="text/css" rel="stylesheet" href="http://makent.trioangle.com/css/edit_profile.css">
          <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.3.0-beta.1/angular-cookies.min.js"></script>

          <script src="http://localhost:3000/angularjs/listings.js"/>
          <script src="js/ng-file-upload.min.js"></script>
</head>


<body class="ng-scope" ng-app="UserProfile" ng-controller="previousTrip">
<div  id="header" class=" navbar navbar-default  ">
  <div  class="navbar-header">
    <a class="navbar-brand" style="position:relative;padding:0"  href="#"><img id="a" src="http://media.gizmodo.co.uk/wp-content/uploads/2014/07/airbnb-logo-230x129.jpg" height=50 width=90> </a>
  </div >
  <ul class="nav navbar-nav navbar-right">
    <li><a id="list-your-space"  class="become" href="" style="border-left:none !important;">
        <!--This should lead to become a host page remaining-->
        <span id="list-your-space" class="btn btn-special list-your-space-btn">
          Become a Host
          </span>
        </a>
    </li>
    <li> <a class="help-toggle link-reset" href=""><!--This should lead to help page remaining-->
      <span class="help-pos">Help</span>
       <i class="help-icon"></i>
    </a></li>
    <li><a class="link-reset trip-drop" href=""> <!--Trip should lead to booking trip pages remaining-->
    <span class="trip-pos"> Trips</span>
    <i class="trips-icon">
        <i class="alert-count fade">0</i>
      </i>
    </a></li>
    <li><a href="" rel="nofollow" class="no-crawl link-reset inbox-icon">
    <span class="msg-icon1">Messages</span>
