        <li><a href="/profile">Profile</a></li>
        <li><a href="/logout">Logout</a></li>
    </ul>
  </li>

</ul>
</div>
 
    <main id="site-content" role="main" >
    <div class="subnav hide-print">
  <div class="page-container-responsive">
    <ul class="subnav-list">
      <li>
         <a href="/Profile" aria-selected="false" class="subnav-item">Dashboard</a>
      </li>
      <li>
        <a ng-click="#" aria-selected="true" class="subnav-item">Your Listings</a>
      </li>
      <li>
         <a href="/trip" aria-selected="false" class="subnav-item">Your Trips</a>
      </li>
      <li>
        <a href="/EditProfile" aria-selected="false" class="subnav-item">Profile</a>
      </li>
      <li>
        <a href="/Account" aria-selected="false" class="subnav-item">Account</a>
      </li>
    </ul>
  </div>
</div>

    <div class="page-container-responsive space-top-4 space-4">
    <div class="row">
      <div class="col-md-3">
        <ul class="sidenav-list">


  <li>
    <a class="sidenav-item active" aria-selected="true" href="#">Your Listings</a>
  </li>
  <li>
    <a class="sidenav-item" aria-selected="false" href="/hostReservations">Your Reservations</a>
  </li>



</ul>      </div>


      <div class="col-md-9">
            <div class="panel" >
                <div class="panel-header">
                  Your Listings
                </div>
                <div class="panel-body">

                 <table class="table table-hover table-condensed" ng-repeat="list in listings" style="border-bottom: 3px solid #000;">
                      <tr>
                        <th>Property Title</th>
                        <td> &nbsp: &nbsp</td><td>{{list.property_title}}</td>
                      </tr>
                      <tr>
                        <th>Property Description</th>
                        <td> &nbsp: &nbsp</td><td>{{list.property_description}}</td>
                      </tr>
                      <tr>
                        <th>Address Line 1</th>
                        <td> &nbsp: &nbsp</td><td>{{list.street_address}}</td>
                      </tr>
                       <!-- <tr>
                      <th>Address Line 2</th><td> &nbsp: &nbsp</td><td>{{list.listing_address2}}</td>
                      </tr> -->
                      <tr>
                      <th>City </th><td> &nbsp: &nbsp</td><td>{{list.city}}</td>
                      
                      </tr>
                      <tr>
                      <th>State </th><td> &nbsp: &nbsp</td><td>{{list.state}}</td>
                      
                      </tr>
                      <tr>
                      <th> Zip Code</th><td> &nbsp: &nbsp</td><td>{{list.zip_code}}</td>
                      
                      </tr>
                      <tr>
                      <th>Host Contact Info </th><td> &nbsp: &nbsp</td><td>{{list.property_owner}}</td>
                       
                      </tr>
                        <tr class="success">
                      <th>House info </th><td> &nbsp: &nbsp</td>
                              <td> 
                                <tr class="warning">
                                  
                                    <th>&nbsp &nbsp  &nbsp Room Type</th>
                                    <td> &nbsp: &nbsp</td>
                                    <td>{{list.room_type}}</td>   
                                </tr>  
                                <tr class="warning">
                                  
                                    <th>&nbsp &nbsp &nbsp Number of Beds</th>
                                    <td> &nbsp: &nbsp</td>
                                    <td>{{list.number_of_beds}}</td>   
                                </tr> 
                                <tr class="warning">
                                  
                                    <th>&nbsp &nbsp &nbsp Number of Guests</th>
                                    <td> &nbsp: &nbsp</td>
                                    <td>{{list.number_of_guests}}</td>   
                                </tr> 
                                <tr class="warning">
                                  
                                    <th>&nbsp &nbsp &nbsp Shared Amenities</th>
                                    <td> &nbsp: &nbsp</td>
                                
                                      <td> 
                                      <span ng-repeat="x in list.shared_amenities">
                                        {{x}} </br></span>
                                      </td>
                                </tr>   
                                <tr class="warning">
                                  
                                    <th>&nbsp &nbsp &nbsp Shared Spaces</th>
                                    <td> &nbsp: &nbsp</td>
                                <td> 
                                      <span ng-repeat="x in list.shared_spaces">
                                        {{x}} </br></span>
                                      </td>
                                </tr>  
                                <tr class="warning">
                                  
                                    <th>&nbsp &nbsp &nbsp Price</th>
                                    <td> &nbsp: &nbsp</td>
                                    <td>{{list.price}}</td>   
                                </tr>    
                              </td>

                      </tr>
                    
                      <tr>
                        <th>Total Revenue</th>
                        <td> &nbsp: &nbsp</td><td>{{list.revenue}}</td>
                      </tr>
                      <tr>
                        <th>Price of Listing</th>
                        <td> &nbsp: &nbsp</td><td>{{list.price}}</td>
                      </tr>
                      <tr>
                        <th>Bid Status</th>
                        <td> &nbsp: &nbsp</td><td>{{list.bid_status}}</td>
                      </tr>
                 </table>   
              </div>
      
             </div>
     </div>


    
   
      </div>
    </div>
               
     </main>


