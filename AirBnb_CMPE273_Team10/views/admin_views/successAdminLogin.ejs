<script>
var timeSpentOnPage;
var app = angular.module('airbnb_admin');
//login
app.controller('airbnb_admin', function($scope, $http) {

	console.log("I AM INSIDE airbnb_admin CONTROLLER ON PAGE: airbnb_admin");
	$scope.invalid_login = true;
	var clicks_on_this_page = 0;
	$(document).click(function(event) {
		var target = event.target || event.srcElement;
		var id = target.id;
		clicks_on_this_page = clicks_on_this_page + 1;
		if(id == ""){
			id = "empty";
		}
		
		var text = $(event.target).text();
	    var clicklog = id;

	    timeSpentOnPage = TimeMe.getTimeOnCurrentPageInSeconds();
		var page_analytics = {
				"clicklog" : clicklog,
				"timeSpentOnPage" : timeSpentOnPage,
				"page_id" : "airbnb_admin_successlogin_page",
				"clicks_on_this_page" : clicks_on_this_page
		};
		
		console.log(page_analytics);
		
		$http({
			method : "POST",
			url : '/logMyEvent',
			data : page_analytics
		}).success(function(data) {

			if (data.statusCode === 200) {
				console.log("MY EVENT HAS BEEN LOGGED ON : airbnb_admin_successlogin_page");
				// console.log(data);
			} else {
				console.log("SOMETHING WENT WRONG");
			}
		});
	});

});




$(window).unload(function() {
	
